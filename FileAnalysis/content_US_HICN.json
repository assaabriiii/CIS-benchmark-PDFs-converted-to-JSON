{
    "custom_item_1": {
        "description": "PII - File contains a HICN",
        "file_extension": "txt",
        "regex": "([^0-9-]|^)([0-9]{3}-?[0-9]{2}-?[0-9]{4}-?(A|[BDE][1-9A-Z]?|[CK][1-9A-Z]|[FJ][1-9]|M[1]?|W[1-9RT]?))([^0-9-]|$)",
        "expect": "HICN",
        "include_paths": "/home",
        "max_size": "50K",
        "only_show": "6"
    },
    "custom_item_2": {
